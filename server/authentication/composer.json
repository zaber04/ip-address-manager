{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "darkaonline/l5-swagger": "^8.5",
        "illuminate/broadcasting": "^10.37",
        "illuminate/database": "^10.37",
        "illuminate/queue": "^10.37",
        "laravel/lumen-framework": "^10.0",
        "tymon/jwt-auth": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Authentication\\": "app/",
            "AuthenticationDatabase\\Factories\\": "database/factories/",
            "AuthenticationDatabase\\Seeders\\": "database/seeders/",

            "Gateway\\": "../gateway/app/",
            "GatewayDatabase\\Factories\\": "../gateway/database/factories/",
            "GatewayDatabase\\Seeders\\": "../gateway/database/seeders/",

            "IpHandler\\": "../ip-handler/app/",
            "IpHandlerDatabase\\Factories\\": "../ip-handler/database/factories/",
            "IpHandlerDatabase\\Seeders\\": "../ip-handler/database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
