{
    "name": "ip-address-manager/gateway",
    "description": "Provides the API gateway microservice",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/console": "*",
        "illuminate/events": "^10.37",
        "illuminate/filesystem": "*",
        "illuminate/support": "*",
        "laravel/lumen-framework": "^10.0",
        "laravel/tinker": "^2.8",
        "league/flysystem": "^3.23",
        "tymon/jwt-auth": "^2.0",
        "zaber04/lumen-api-resources": "dev-master"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Gateway\\": "app/",
            "GatewayDatabase\\Factories\\": "database/factories/",
            "GatewayDatabase\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/zaber04/lumen-api-resources.git"
        }
    ]
}
